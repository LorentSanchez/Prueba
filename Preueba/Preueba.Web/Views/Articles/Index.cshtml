@model IEnumerable<Prueba.Web.Models.Articles>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ArticlesName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClassificationArts)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Brands)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ArticlesName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ClassificationArts.NameArtc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Brands.BrandName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ArticlesId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ArticlesId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ArticlesId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
